{
  "expo": {
    "name": "react-native-test",
    "slug": "react-native-test",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/favicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Krub/Krub-Regular.ttx",
            "./assets/fonts/Krub/Krub-Medium.ttx",
            "./assets/fonts/Krub/Krub-SemiBold.ttx"
          ]
        }
      ]
    ]
  }
}