{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "entryFile": "main",
  "sourceRoot": "apps/api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api",
  "projects": {
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "app-config": {
      "type": "library",
      "root": "libs/app-config",
      "entryFile": "index",
      "sourceRoot": "libs/app-config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/app-config/tsconfig.lib.json"
      }
    },
    "logger": {
      "type": "library",
      "root": "libs/logger",
      "entryFile": "index",
      "sourceRoot": "libs/logger/src",
      "compilerOptions": {
        "tsConfigPath": "libs/logger/tsconfig.lib.json"
      }
    },
    "utility": {
      "type": "library",
      "root": "libs/utility",
      "entryFile": "index",
      "sourceRoot": "libs/utility/src",
      "compilerOptions": {
        "tsConfigPath": "libs/utility/tsconfig.lib.json"
      }
    },
    "filter": {
      "type": "library",
      "root": "libs/filter",
      "entryFile": "index",
      "sourceRoot": "libs/filter/src",
      "compilerOptions": {
        "tsConfigPath": "libs/filter/tsconfig.lib.json"
      }
    },
    "constant": {
      "type": "library",
      "root": "libs/constant",
      "entryFile": "index",
      "sourceRoot": "libs/constant/src",
      "compilerOptions": {
        "tsConfigPath": "libs/constant/tsconfig.lib.json"
      }
    },
    "exception": {
      "type": "library",
      "root": "libs/exception",
      "sourceRoot": "libs/exception/src",
      "compilerOptions": {
        "tsConfigPath": "libs/exception/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "validation": {
      "type": "library",
      "root": "libs/validation",
      "entryFile": "index",
      "sourceRoot": "libs/validation/src",
      "compilerOptions": {
        "tsConfigPath": "libs/validation/tsconfig.lib.json"
      }
    },
    "middleware": {
      "type": "library",
      "root": "libs/middleware",
      "entryFile": "index",
      "sourceRoot": "libs/middleware/src",
      "compilerOptions": {
        "tsConfigPath": "libs/middleware/tsconfig.lib.json"
      }
    },
    "script": {
      "type": "application",
      "root": "apps/script",
      "entryFile": "main",
      "sourceRoot": "apps/script/src",
      "compilerOptions": {
        "tsConfigPath": "apps/script/tsconfig.app.json"
      }
    },
    "dayjs": {
      "type": "library",
      "root": "libs/dayjs",
      "entryFile": "index",
      "sourceRoot": "libs/dayjs/src",
      "compilerOptions": {
        "tsConfigPath": "libs/dayjs/tsconfig.lib.json"
      }
    }
  }
}