{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "strict": true,
    "strictPropertyInitialization": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@lib/app-config": [
        "libs/app-config/src"
      ],
      "@lib/app-config/*": [
        "libs/app-config/src/*"
      ],
      "@lib/logger": [
        "libs/logger/src"
      ],
      "@lib/logger/*": [
        "libs/logger/src/*"
      ],
      "@lib/utility": [
        "libs/utility/src"
      ],
      "@lib/utility/*": [
        "libs/utility/src/*"
      ],
      "@lib/filter": [
        "libs/filter/src"
      ],
      "@lib/filter/*": [
        "libs/filter/src/*"
      ],
      "@lib/constant": [
        "libs/constant/src"
      ],
      "@lib/constant/*": [
        "libs/constant/src/*"
      ],
      "@lib/exception": [
        "libs/exception/src"
      ],
      "@lib/exception/*": [
        "libs/exception/src/*"
      ],
      "@lib/database": [
        "libs/database/src"
      ],
      "@lib/database/*": [
        "libs/database/src/*"
      ],
      "@lib/validation": [
        "libs/validation/src"
      ],
      "@lib/validation/*": [
        "libs/validation/src/*"
      ],
      "@lib/middleware": [
        "libs/middleware/src"
      ],
      "@lib/middleware/*": [
        "libs/middleware/src/*"
      ],
      "@lib/dayjs": [
        "libs/dayjs/src"
      ],
      "@lib/dayjs/*": [
        "libs/dayjs/src/*"
      ]
    }
  }
}